#include <stdio.h>
#include <string.h>
 
int main( )
{
   char source[ ] = "karthi" ;
   char destin[ ]= "natarajan" ;
 
   printf ( "\nSource string = %s", source ) ;
   printf ( "\nTarget string = %s", destin ) ;
 
   strcat ( destin, source ) ;
 
   printf ( "\nTarget string after strcat( ) = %s", destin ) ;
   return 0;
}


#include <stdio.h>
#include <string.h>
 
int main( )
{
   char source[20 ] = "karthi";
   char desn[20]= "natarajan";
   printf ( "\nSource string = %s", source );
   printf ( "\nDestination string = %s", desn );
   strcpy (desn,source);
   printf ( "\nString after strcpy( ) = %s",desn);
   return 0;
}


#include<stdio.h>
#include<string.h>

int main()
{
    int n;
    char s[100]="karthinatarajan";
    n=strlen(s);
    printf("\nThe size of the string entered is %d",n);
    return 0;
}


#include<stdio.h>
#include<string.h>

int main()
{ int n;
    char abc[20]="karthi";
    char bcd[20]="natarajan";
    n=strcmp(abc,bcd);
    if(n==0)
    {
        printf("\nEntered string is same");
        
    }
    else
    {
        printf("\nEntered string is different");
        
    }
    return 0;
    
}



