#include<stdio.h>

int main()
{
    int a[3][3][3],b[3][3][3],sum[3][3][3];
    int i,j,k;
  
    
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            for(k=0;k<3;k++)
            {
                scanf("%d",&a[i][j][k]);
            }
        }
        
    }
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            for(k=0;k<3;k++)
            {
                scanf("%d",&b[i][j][k]);
            }
        }
    }
    for(i=0;i<3;i++)
    {
        for(j=0;j<3;j++)
        {
            for(k=0;k<3;k++)
            {
                sum[i][j][k] = a[i][j][k] + b[i][j][k];
            
                printf("%d\t",sum[i][j][k]);
            }
           
        }
         printf("\n");
    }
    return 0;
}


