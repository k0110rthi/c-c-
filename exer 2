
#include<stdio.h>

int main()
{
    
    
    int i,j;
   
        scanf("%d%d",&i,&j);
        if(i == j)
        {
                printf("\nNumbers are equal");

        }     
        else
        {
              printf("\nNumbers are not equal");
        }    
return 0;
} 


#include<stdio.h>

int main()
{
    
    
    int i;
   
        scanf("%d",&i);
        if(i > 0)
        {
                printf("\nNumbers is positive");

        }     
        else
        {
              printf("\nNumbers is negative");
        }    
return 0;
}

#include<stdio.h>

int main()
{
    
    
    int a,b,c;
   
        scanf("%d%d%d",&a,&b,&c);
        if(a==b || a==c || b==c)
        {
                printf("\nThis number is an isocles triangle");

        }  
        else if(a==b || b==c || c==a)
        {
              printf("\nThis number is equilateral triangle"); 
         }
        else
        {
              printf("\nThe number is not an scalene triangle");
        }    
return 0;
}



#include<stdio.h>

int main()
{
    
    
    char i;
   
        scanf("%c",&i);
        if((i>'a' && i<'z') || (i>'0' && i<'9'))
        {
                printf("\nThis is not  a special character");

        }     
        else
        {
              printf("\nThis is a special character");
        }    
return 0;
}


#include<stdio.h>
int main()
{
    int cust_id,unit,i;
    float s,n,p;
    char cust_name[20];
    scanf("%d%d%s",&cust_id,&unit,&cust_name);
    printf("\nCustomer IDNO : %d",cust_id);
    printf("\nUnits : %d",unit);
    printf("\nCustomer name : %s",cust_name);
    if(unit < 200)
    {
        p=unit*1.20;
        printf("\nAmount charges @1.20 per unit = %.2f",p);
    }
    else if(unit >= 200 && unit <= 400)
    {
        p=unit*1.50;
        printf("\nAmount charges @1.50 per unit = %.2f",p);
        if(p > 400)
        {
            s=unit*0.15;
            n=s+p;
            printf("\nSurcharge : %.2f",s);
            printf("\nNet amount to be paid : %.2f",n);
        }
        else
        {
            printf("\n");
        }
    }
    else if(unit >= 400 && unit <= 600)
    {
        p=unit*2;
        printf("\nAmount charges @2.00 per unit = %.2f",p);
    
     if(p > 400)
     {
        s=unit*0.15;
        n=s+p;
        printf("\nSurcharge : %.2f",s);
        printf("\nNet amount to be paid : %.2f",n);
      }
      else
     {
        printf("\n");
     }
    }
    else if(unit >= 600)
    {
        p=unit*2;
        printf("\nAmount charges @2.00 per unit = %.2f",p);
    
     if(p > 400)
     {
        s=unit*0.15;
        n=s+p;
        printf("\nSurcharge : %.2f",s);
        printf("\nNet amount to be paid : %.2f",n);
     }
     else
     {
        printf("\n");
     }
    }
    else
    {
        printf("\nWrong input");
    }
    return 0;
}


