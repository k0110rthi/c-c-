
#include<stdio.h>

int main()
{
    
    
    int i,j;
   
        scanf("%d%d",&i,&j);
        if(i == j)
        {
                printf("\nNumbers are equal");

        }     
        else
        {
              printf("\nNumbers are not equal");
        }    
return 0;
} 


#include<stdio.h>

int main()
{
    
    
    int i;
   
        scanf("%d",&i);
        if(i > 0)
        {
                printf("\nNumbers is positive");

        }     
        else
        {
              printf("\nNumbers is negative");
        }    
return 0;
}

#include<stdio.h>

int main()
{
    
    
    int a,b,c;
   
        scanf("%d%d%d",&a,&b,&c);
        if(a==b || a==c || b==c)
        {
                printf("\nThis number is an isocles triangle");

        }  
        else if(a==b || b==c || c==a)
        {
              printf("\nThis number is equilateral triangle"); 
         }
        else
        {
              printf("\nThe number is not an scalene triangle");
        }    
return 0;
}



#include<stdio.h>

int main()
{
    
    
    char i;
   
        scanf("%c",&i);
        if((i>'a' && i<'z') || (i>'0' && i<'9'))
        {
                printf("\nThis is not  a special character");

        }     
        else
        {
              printf("\nThis is a special character");
        }    
return 0;
}


